<%= form_for @user, url: users_path(@user), html: { method: :put } do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>


  <%= f.submit "Edit Changes" %>
<% end %>