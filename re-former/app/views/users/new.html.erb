<form action="/users" method="post" accept-charset="UTF-8" >
  <label for="username"> Type in username</label> <br> 
 <input type="username" name="user[username]" id="username" maxlength="8">

 <label for="email"> Type email</label> <br> 
 <input type="email" name="user[email]" id="email">

 <label for="password"> Type password</label> <br> 
 <input type="password" name="user[password]" id="password" maxlength="10">
  
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  
  <input type="submit" value="Submit"> 
</form>


